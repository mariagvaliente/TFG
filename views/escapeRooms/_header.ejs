<header class="" id="mainHeader">

    <div class="right">
        <% if (!session.user) { %>
            <a href="/">Login</a>
        <% } else { %>
            <%= session.user.username %>
        <% } %>
    </div>
    <div class="left">
        <% if (session.user) { %>
            <a href="/?_method=DELETE">Logout</a>
        <% } %>
    </div>

</header>
