<header class="main" id="mainHeader">

    <div class="right">
        <% if (!session.user) { %>
            <a href="/">Login</a>
        <% } else { %>
            <a href="/users/<%= session.user.id %>"><%= session.user.username %></a>

        <% } %>
    </div>
    <div class="left">
        <% if (session.user) { %>
            <a href="/?_method=DELETE">Logout</a>
        <% } %>
    </div>

</header>
<h1>
    Editar Escape Room:
</h1>

<form method="post" action="/escapeRooms/<%= escapeRoom.id %>?_method=PUT" enctype="multipart/form-data">
    <% include _form.ejs %>
</form>