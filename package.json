{
  "name": "web",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "heroku local:start",
    "start_old": "node ./bin/www",
    "dev": "DEBUG=TFG  nodemon bin/www -e sass,js --exec \"npm run css && npm run lint && node\" && xdg-open http://localhost:3000 ",
    "dev-windows": "nodemon bin/www -e sass,js --exec \"npm run css && npm run lint && node\" && start http://localhost:3000",
    "test": "jest",
    "migrate_local": "sequelize db:migrate --url sqlite://$(pwd)/escaperoom.sqlite",
    "migrate_heroku": "heroku run ./node_modules/.bin/sequelize db:migrate --url \\$DATABASE_URL",
    "seed_heroku": "heroku run ./node_modules/.bin/sequelize db:seed:all --url \\$DATABASE_URL",
    "seed_local": "sequelize db:seed:all --url sqlite://$(pwd)/escaperoom.sqlite",
    "reset_heroku": "heroku pg:reset DATABASE",
    "reset_local": "rm -rf escaperoom.sqlite",
    "open_heroku": "heroku open",
    "css": "node-sass public/stylesheets/style.sass public/stylesheets/style.css",
    "lint": "eslint . "
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cloudinary": "^1.9.1",
    "connect-session-sequelize": "^6.0.0",
    "cookie-parser": "^1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^7.0.0",
    "ejs": "^2.6.1",
    "eslint": "^5.15.1",
    "express": "^4.16.4",
    "express-flash": "^0.0.2",
    "express-partials": "^0.3.0",
    "express-session": "^1.15.6",
    "fs": "^0.0.1-security",
    "http-errors": "~1.6.2",
    "json-2-csv": "^3.5.0",
    "method-override": "^3.0.0",
    "morgan": "^1.9.1",
    "multer": "^1.3.0",
    "node-sass": "^4.11.0",
    "node-sass-middleware": "0.11.0",
    "nodemon": "^1.18.10",
    "pg": "7.4.0",
    "sass": "^1.17.0",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0"

  },
  "devDependencies": {
    "jest": "^24.1.0",
    "sqlite3": "^4.0.6"
  }
}
